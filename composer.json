{
  "name": "heimrichhannot/contao-privacy-protocol-bundle",
  "type": "contao-bundle",
  "description": "This bundle add a privacy logging entity to contao as required by the General Data Protection Regulation.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^8.1",
    "composer-runtime-api": "^2.0",
    "contao/core-bundle": "^4.13 || ^5.0",
    "heimrichhannot/contao-utils-bundle": "^2.241 || ^3.6",
    "symfony/http-foundation": "^5.4 || ^6.0",
    "symfony/http-kernel": "^5.4 || ^6.0",
    "symfony/security-core": "^5.4 || ^6.0",
    "symfony/translation-contracts": "^1.0 || ^2.0 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\PrivacyProtocolBundle\\": "src/"
    }
  },
  "conflict": {
    "heimrichhannot/contao-privacy": "*",
    "heimrichhannot/contao-privacy-bundle": "*"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\PrivacyProtocolBundle\\ContaoManager\\Plugin"
  },
  "require-dev": {
    "rector/rector": "^1.2 || ^2.0",
    "contao/contao-rector": "dev-main",
    "symplify/easy-coding-standard": "^12.5",
    "phpstan/phpstan": "^1.12 || ^2.0",
    "phpstan/phpstan-symfony": "^1.4 || ^2.0",
    "contao/manager-plugin": "^2.13"
  }
}
